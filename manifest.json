{
  "manifest_version": 2,

  "name": "Memrise dictionary",
  "description": "Memrise dictionary",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "app.html"
  }
}
